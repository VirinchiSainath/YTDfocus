{
	"name": "YTDfocus",
	"version": "1.0",
	"manifest_version": 2,
	"description": "To help you focus when using Youtube.",
	"browser_action": {
		"default_popup": "mypopup.html"
	},
	"content_scripts": [
		{
			"matches" : ["http://www.youtube.com/", "https://www.youtube.com/"],
			"js": ["script.js"]
		}
	]
}
